cc		 = gcc
CFLAGS	 = -Wall -Wextra -g
LDFLAGS  = -pthread
DEPS	 = server.c queue.c queue.h request.c request.h
OBJFILES = server.o queue.o request.o
TARGET	 = server

all: $(TARGET)

$(TARGET): $(OBJFILES) $(DEPS)
	$(CC) $(CFLAGS) -o $(TARGET) $(OBJFILES) $(LDFLAGS)

clean:
	rm -f $(OBJFILES) $(TARGET) *~